  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="9.0.5" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Localization\Platform\*.json" />
    <Content Remove="Localization\Platform\*.json" />
  </ItemGroup>

</Project>
